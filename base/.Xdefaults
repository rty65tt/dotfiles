! FONTS SETTINGS
# urxvt --help 2>&1| sed -n '/:  /s/^ */! URxvt*/gp'
!URxvt*termName:	screen-256color
!URxvt.font:		9x15
!URxvt.bold.font:		9x15Bold
!URxvt.font:		xft:terminus:size=11
!URxvt.bold.font:	xft:terminus:size=11:bold
!URxvt.font:		-*-terminus-medium-r-*--16-160-72-72-c-80-*-*
!URxvt.bold.font:	-*-terminus-bold-r-*--16-160-72-72-c-80-*-*
URxvt.font:		xft:xos4 Terminess Powerline:size=11
!URxvt.bold.font:	xft:xos4 Terminess Powerline:size=11:bold
!URxvt.font:		xft:Ubuntu Mono:size=11
!URxvt.font:		xft:Monospace:size=11
!URxvt*letterSpace: -1

URxvt.geometry:             90x30
URxvt.transparent:          false
! URxvt.urgentOnBell:         true
! URxvt.visualBell:           true
!URxvt.loginShell:           true
!URxvt.internalBorder:       3
URxvt.lineSpace: 0

URxvt.depth:		32
URxvt.background:	[80]#000000
URxvt*scrollBar:	false
URxvt.scrollStyle: 	rxvt
URxvt*mouseWheelScrollPage:	false
URxvt*saveLines:	5000
URxvt*underlineColor:  #3a3a3a

URxvt.scrollBar: true
URxvt.scrollBar_right: true
URxvt.scrollBar_floating: true
URxvt.scrollstyle: rxvt
